<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="@color/litegrey"
	android:id="@+id/menuitems">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/home1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/home"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="Home"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/zpay1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/zpay"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="Z PAY"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/giftcards1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/giftcards"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="GIFTCARDS"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/myfavourites1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/favourite"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="MY FAVOURITES"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/myfriends1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/myfriends"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="MY FRIENDS"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/talktous1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/chat"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="TALK TO US"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/rewards1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/rewards"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="REWARDS"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/settings1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/settings"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="SETTINGS"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal"
		android:paddingLeft="5dip" android:paddingTop="3dip"
		android:paddingRight="3dip" android:background="@drawable/gradient_bg"
		android:id="@+id/logout1">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:gravity="center|center_horizontal">
			<ImageView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:src="@drawable/logout"
				android:id="@+id/menuimage" />
			<TextView android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:text="LOGOUT"
				android:singleLine="true" android:textColor="@color/white"
				android:id="@+id/menutext" android:textSize="13dip" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="1dip" android:background="@color/dividerblack" />
</LinearLayout>
